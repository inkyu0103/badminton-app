{
  "extends": ["next", "next/core-web-vitals", "prettier"],
  "plugins": ["no-relative-import-paths"],
  "rules": {
    "arrow-body-style": ["error", "as-needed"],
    "import/no-anonymous-default-export": "off",
    "no-relative-import-paths/no-relative-import-paths": [
      "error",
      {
        "rootDir": "frontend",
        "prefix": ""
      }
    ]
  }
}
